FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Laminar viscosity
nu              nu [0 2 -1 0 0 0 0] 1e-06;

// Thermal expansion coefficient
beta            beta [-1 3 0 0 0 0 0] 1e-03;

// Reference temperature
DRef            DRef [1 -3 0 0 0 0 0] 1000;

//Heavy fluid density
Dmax		Dmax [1 -3 0 0 0 0 0] 1005.1;

// Laminar Prandtl number
Sc              Sc [0 0 0 0 0 0 0] 7;

// Turbulent Prandtl number
Sct             Sct [0 0 0 0 0 0 0] 7;

// ************************************************************************* //
