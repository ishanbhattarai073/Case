FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue D 1000
);

regions
(
    boxToCell
    {
        box (0 0 0) (0.5 0.15 0.25);

        fieldValues
        (
            volScalarFieldValue D 1005.1
        );
    }
);

